<Page x:Class="DemoTranning.Pages.RegisterPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DemoTranning.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="RegisterPages">

    <Grid>
        <StackPanel Orientation="Vertical" Height="400" Width="400">
            <TextBox/>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                
                <Button Content="Регистрация" x:Name="RegisterButton" Click="RegisterButton_Click"/>
                <Button Content="Назад" x:Name="BackButton" Click="BackButton_Click"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Page>
